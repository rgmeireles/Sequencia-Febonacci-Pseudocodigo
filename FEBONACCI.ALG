ALGORITMO "FEBONACCI"
VAR
V1, V2, V3, LIMITE, CONTADOR: INTEIRO
INICIO
ESCREVA ("QUANTOS TERMOS VOCE DESEJA QUE SEJA EXIBIDO? ")// NO CASO SERÁ 1000
LEIA (LIMITE)
V1 <- 34
V2 <- 55
CONTADOR <- 0
ENQUANTO (CONTADOR<LIMITE) FACA
V3 <- V1 + V2
V1 <- V2
V2 <- V3
ESCREVA  (V3)
CONTADOR <- CONTADOR + 1
FIMENQUANTO
FIMALGORITMO
